{
  "require": {
    "php": ">=8.2",
    "nette/schema": "^1.3",
    "orhanerday/open-ai": "^5.2",
    "guzzlehttp/guzzle": "^7.0",
    "leafs/leaf": "^3.7",
    "leafs/cors": "^1.2",
    "leafs/logger": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "App\\": "./src/App",
      "Chatwoot\\": "./src/Chatwoot",
      "OpenAi\\": "./src/OpenAi",
      "Schema\\": "./src/Schema"
    }
  },
  "require-dev": {
    "vlucas/phpdotenv": "^5.6",
    "phpstan/phpstan": "^1.12"
  },
  "scripts": {
    "phpstan:analyse": "vendor/bin/phpstan analyse --level=6 --memory-limit=-1 src"
  }
}
